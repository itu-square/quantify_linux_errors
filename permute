    depends on PANEL && PANEL_PROFILE="0" && PANEL_LCD="1"
    int "Number of lines on the LCD (1-2)"
    range 1 2
    default 2
config PANEL_LCD_WIDTH
    depends on PANEL && PANEL_PROFILE="0" && PANEL_LCD="1"
    int "Number of characters per line on the LCD (1-40)"
    range 1 40
    default 40
