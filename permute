    def_bool y
    depends on (!SELECT_MEMORY_MODEL && ARCH_DISCONTIGMEM_ENABLE) || DISCONTIGMEM_MANUAL
config SPARSEMEM
    def_bool y
    depends on (!SELECT_MEMORY_MODEL && ARCH_SPARSEMEM_ENABLE) || SPARSEMEM_MANUAL
config FLATMEM
    def_bool y
    depends on (!DISCONTIGMEM && !SPARSEMEM) || FLATMEM_MANUAL
